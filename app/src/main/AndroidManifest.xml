        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AnatomiTubuhManusia">
        <activity
            android:name=".activity.halaman_utama.HalamanUtamaActivity"
            android:exported="true"
            android:theme="@style/Theme.AnatomiTubuhManusia.BlueNoActionBar"
